/* reset */
page {
  background-color: #F8F8F8;
  height: 100%;
  font-size: 32rpx;
  line-height: 1.6;
}
checkbox, radio{
  margin-right: 10rpx;
}
button{
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
button::after {
  border: none;
}
form{
  width: 100%;
}
input {
  width: 100%;
}

/* lib */
.strong{
  font-weight: bold;
}
.tc{
  text-align: center;
}

/* page */
.container {
  display: flex;
  flex-direction: column;
  min-height: 100%;
  justify-content: space-between;
  font-size: 32rpx;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.page-head{
  padding: 20rpx 10rpx 80rpx;
  text-align: center;
}
.page-head-title{
  display: inline-block;
  padding: 0 40rpx 20rpx 40rpx;
  font-size: 32rpx;
  color: #BEBEBE;
}
.page-head-line{
  margin: 0 auto;
  width: 120rpx;
  height: 5rpx;
  background-color: #D8D8D8;
}
.page-head-desc{
  padding-top: 20rpx;
  color: #9B9B9B;
  font-size: 32rpx;
}

.page-body {
  width: 100%;
  flex-grow: 1;
  overflow-x: hidden;
}
.page-body-wrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}
.page-body-wording {
  text-align: center;
  padding: 200rpx 100rpx;
}
.page-body-info {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #fff;
  width: 100%;
  padding: 50rpx 0 150rpx 0;
}
.page-body-title {
  margin-bottom: 100rpx;
  font-size: 32rpx;
}
.page-body-text {
  font-size: 30rpx;
  line-height: 26px;
  color: #ccc;
}
.page-body-text-small {
  font-size: 24rpx;
  color: #000;
  margin-bottom: 100rpx;
}

.page-section{
  width: 100%;
  margin-bottom: 20rpx;
}
.page-section_center{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.page-section:last-child{
  margin-bottom: 0;
}
.page-section-gap{
  box-sizing: border-box;
  padding: 0 30rpx;
}
.page-section-spacing{
  box-sizing: border-box;
  padding: 0 80rpx;
}
.page-section-title{
  font-size: 28rpx;
  font-family: 'Courier New', Courier, monospace;
  color: #999999;
  margin-bottom: 10rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  text-align: center
}
.page-section-gap .page-section-title{
  padding-left: 0;
  padding-right: 0;
}

.overlap-image {
  position: absolute;
  width: 80rpx;
  height: 80rpx;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%)
}
.float-bar {
  background-color: #0084FF;
  color: white;
  position: fixed;
  width: 100%;
  height: 100rpx;
  top: 100%;
  left: 0;
  z-index: 100;
  transform: translateY(-100%);
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.bar-item {
  color: white;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.delete-button {
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  width: 200rpx;
  height: 100%;
  top: 0rpx;
  right: -200rpx;
  background: #F56C6C;
  color: #fff;
}

.navigator-hover {
  opacity: 0.5;
}

.placeholder {
  color:#F56C6C;
  font-style: italic;
}

@font-face {font-family: "iconfont";
  src: url('iconfont.eot?t=1590323116934'); /* IE9 */
  src: url('iconfont.eot?t=1590323116934#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('iconfont.woff?t=1590323116934') format('woff'),
  url('iconfont.ttf?t=1590323116934') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('iconfont.svg?t=1590323116934#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-image:before {
  content: "\e632";
}

.icon-video:before {
  content: "\e6c5";
}

.icon-text:before {
  content: "\e61c";
}

.icon-document:before {
  content: "\e6a7";
}

.overlap-icon {
  position: absolute;
  width: 80rpx;
  height: 80rpx;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  display: flex;
  align-items: center;
  justify-content: center;
  border: dashed 4rpx #C0C4CC;
  color: #C0C4CC;
  background-color: #F2F6FC;
  box-sizing: border-box;
}


.default-button {
  width: 90%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #0084FF;
  color: white;
}

.mask {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 101;
  background-color: black;
  opacity: 0.3;
}
