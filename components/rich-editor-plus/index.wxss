.editor-wrapper {
  width: 750rpx;
  background: #F2F6FC;
  padding: 20rpx;
  box-sizing: border-box;
  margin-bottom: 150rpx;
}

.editor-add-wrapper {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 300rpx;
  height: 95rpx;
  margin: 0 auto;
}

.editor-add {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 75rpx;
  height: 75rpx;
  border: dashed 4rpx #C0C4CC;
  color: #C0C4CC;
  box-sizing: border-box;
}

.editor-node-wrapper {
  position: relative;
  width: 650rpx;
  margin: 0 auto;
}

.editor-textarea {
  width: 650rpx;
  border: solid 2rpx #E4E7ED;
  background: #fff;
  min-height: 100rpx;
  font-size: 32rpx;
  box-sizing: border-box;
}

.editor-delete {
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  top: -18rpx;
  right: -18rpx;
  border-radius: 50%;
  background: #F56C6C;
  color: #fff;
}
.editor-image {
  display: block;
  width: 650rpx;
}

.editor-video {
  width: 100%;
  height: 500rpx;
}