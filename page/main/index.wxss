@import "../common/index.wxss";

.page-container {
  padding-bottom: 10rpx;
  overflow-x: hidden;
}
.image-box {
  display: block;
}
.main-image {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
.contents {
  margin-top: 20rpx;
  background-color: white;
  border-radius: 20rpx;
  height: 500rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
  padding-left: 70rpx;
  padding-right: 20rpx;
}
.item-box {
  width: 300rpx;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

.item-box image {
  margin-right: 20rpx;
  height: 60rpx;
  width: 60rpx;
}